		    {
		        Dimi_SetPlayerHealth( playerid, health - 3.0);
		        ApplyAnimation( playerid, "SWEET", "Sweet_injuredloop", 4.0, 1, 0, 0, 0, 0, 1);
		        GameTextForPlayer( playerid, "~r~Niste uzeli svoju dozu,vase tijelo zeli drogu!", 2000, 6);
