		    if( igrac != INVALID_PLAYER_ID ) {
		        if( PlayerInfo[ igrac ][ xLevel ] < 3 ) return SendErrorMessage( playerid, "Igrac mora biti 3+ da bi mu popravio vozilo." );
				if( igrac == playerid ) return SendErrorMessage( playerid, "Ne mozes sam sebi." );
				if( IsPlayerNearPlayer( 5.0, playerid, igrac )&& IsPlayerInAnyVehicle( igrac ) ) {
