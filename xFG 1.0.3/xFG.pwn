			{
				if( GetFactionType( i ) == ORG_TIP_TAXI && taxiDuty[ i ] == true )
					SendClientMessageEx(i, -1, "%s {FBE600}je prihvatio klijenta"col_white" %s.", ImeIgraca( playerid ), ImeIgraca( izbor ) );
