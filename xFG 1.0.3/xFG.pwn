		SCM( playerid, SVETLOPLAVA, "___________________________________________________________________");
		SendUsageMessage( playerid, "/taxi [ opcija ]");
     	SCM( playerid, BELA, "[Opcije] duty, ponudivoznju, uzmiklijenta");
		SCM( playerid, SVETLOPLAVA, "___________________________________________________________________");
	}
	return 1;
}

