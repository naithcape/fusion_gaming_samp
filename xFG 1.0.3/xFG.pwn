		    if( NaUtrci[ playerid ] ) return SendErrorMessage( playerid, "Ne mozes dok si na trci." );
		    if( PlayerInfo[ igrac ][ xAdmin ] >= 1 ) return SendErrorMessage( playerid, "Ne mozes se portati do Admina." );
