			}
		}

		format( globalstring, sizeof( globalstring ), "***[UNLOAD trke] %s", ImeIgraca( playerid ) );
		AdminPoruka( 0x6495EDAA, globalstring );
		SCM( playerid, ZUTA, "Prekinuo si event." );
