	if( phonenumb == 777 ) {
	    if( IsPlayerInAnyVehicle( playerid ) ) return SendErrorMessage( playerid, "Ne mozete zvati iz vozila!");
		foreach( Player, i )
		{
			if( GetFactionType( i ) == ORG_TIP_TAXI && taxiDuty[ i ] == true )
