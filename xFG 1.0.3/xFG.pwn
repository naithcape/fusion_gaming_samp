stock IsBrojKombinacija( kombinacijaid, brojvaluta ) {

	if( kombinacijaid == 1 ) {
	
	    if( JelZero( brojvaluta ) ) return 1;
	}
	else if( kombinacijaid == 2) {
	
	    if( JelMala( brojvaluta ) ) return 1;
	}
	else if( kombinacijaid == 3 ) {
	
	    if( JelOrfa( brojvaluta ) ) return 1;
	}
	return 0;
}

stock JelZero( brojvaluta ) {

	//0,2,3,4,7,12,15,18,19,21,22,25,26,28,29,32,35
	new bvl = brojvaluta;
	if( bvl == 0 || bvl == 2 || bvl == 3 || bvl == 4 || bvl == 7 || bvl == 12 || bvl == 15 || bvl == 18 || bvl == 19 ) return 1;
	else if( bvl == 21 || bvl == 22 || bvl == 25 || bvl == 26 || bvl == 28 || bvl == 29 || bvl == 32 || bvl == 35 ) return 1;
	return 0;
}

stock JelMala( brojvaluta ) {

	//5,8,11,10,13,16,23,24,27,30,33,36
	new bvl = brojvaluta;
	if( bvl == 5 || bvl == 8 || bvl == 11 || bvl == 10 || bvl == 13 || bvl == 16 || bvl == 23 || bvl == 24 || bvl == 27 ) return 1;
	else if( bvl == 30 || bvl == 33 || bvl == 36 ) return 1;
	return 0;
}

stock JelOrfa( brojvaluta ) {

	//1,6,9,14,17,20,31,34
	new bvl = brojvaluta;
	if( bvl == 1 || bvl == 6 || bvl == 9 || bvl == 14 || bvl == 17 || bvl == 20 || bvl == 31 || bvl == 34 ) return 1;
	return 0;
}

