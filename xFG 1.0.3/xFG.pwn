		if( !IsPlayerNearPlayer( 5.0, playerid, igrac ) )  return SendErrorMessage( playerid, "Igrac nije blizu vas." );
        if(	strcmp( ImeIgraca( igrac ), VehicleInfo[ autoid ][ vOwner ], true) == 0 )
		{
		    if( igrac == playerid ) return SendErrorMessage( playerid, "Ne mozes sam sebi.");

            VehicleInfo[ autoid ][ vTeh ] = 1; SaveVehicle( autoid );
			SendJobMessage( playerid, "Pregledali ste vozilo %s-a i zakljucili da je tehnicki ispravno.", ImeIgraca( igrac ), kolicina );
            SendClientMessageEx( igrac, SVETLOPLAVA, "* Mehanicar %s vam je pregledao vozilo i rekao da je tehnicki ispravno.", ImeIgraca( playerid ) );
		}
		else return SendErrorMessage( playerid, "Igrac nije vlasnik tog vozila." );
 	    return 1;
 	}
	else {
	    SCM( playerid, SVETLOPLAVA, "___________________________________________________________________");
		SendUsageMessage( playerid, "/mechanic [opcija]");
     	SCM( playerid, BELA, "[Opcije] buyparts, checkparts, popravimotor, popraviokvir, popravigume");
