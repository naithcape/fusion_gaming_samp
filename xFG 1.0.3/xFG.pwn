	new string[ 30 ];

	for( new i = 0; i < 10; i++) {

	    format( string, sizeof( string ), "par1ID%d", i );
		INI_Int( string, StariParovi[ i ][ parID ][ 0 ] );
		
		format( string, sizeof( string ), "par2ID%d", i );
