        SCM( playerid, BELA, "[Opcije] duty, refill, mot(Tehnicki Pregled)");
		SCM( playerid, SVETLOPLAVA, "___________________________________________________________________");
	}
	return 1;
}

CMD:natocicisternu( playerid, params[] ) {

