        SendUsageMessage( playerid, "/w [ ID/DeoImena ] [ text ]");
        return 1;
    }
    if( PlayerCuffed[ playerid ] >= 1 ) return SendErrorMessage( playerid, "Ne mozes koristiti komandu kad si cuffovan ili tazovan.");
