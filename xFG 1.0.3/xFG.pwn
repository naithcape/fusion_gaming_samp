global_tds(playerid,response)
{
	switch(response)
	{
		  case false:
		  {
				  for(new i = 0; i < 19; ++i)
				  {
						 TextDrawHideForPlayer(playerid,Global_TDs[SM_TDs][i]);
				  }
		  }
		  case true:
		  {
				  for(new i = 0; i < 19; ++i)
				  {
						 TextDrawShowForPlayer(playerid,Global_TDs[SM_TDs][i]);
				  }
		  }
	}
	return (true);
}

sm_info_tds(playerid,response)
{
    switch(response)
	{
		  case false:
		  {
				  for(new i = 0; i < 11; ++i)
				  {
						 PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
				  }
				  
				  for(new i = 16; i < 20; ++i)
				  {
						 PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
				  }
		  }
		  case true:
		  {
				  for(new i = 0; i < 11; ++i)
				  {
						 PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
				  }
				  
				  for(new i = 16; i < 20; ++i)
				  {
						 PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
				  }
		  }
	}
	return (true);
}

stock hide_hunger(playerid)
{
       for(new i = 11; i <  20; ++i)
	   {
			   PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
       }
       return (true);
}

stock show_hunger(playerid)
{
       for(new i = 11; i <  16; ++i)
	   {
			   PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
       }
       PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][18]);
       PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][19]);
       return (true);
}

stock sm_update_hunger(playerid)
{
    if(PlayerInfo[ playerid ][ xHunger ] >= (80) && PlayerInfo[ playerid ][ xHunger ] <= (100))
  	{
          for(new i = 11; i <= 16; ++i)
		  {
			   PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
          }
  	}
  	if(PlayerInfo[ playerid ][ xHunger ] >= (60) && PlayerInfo[ playerid ][ xHunger ] <= (80))
  	{
          for(new i = 11; i < 15; ++i)
		  {
			   PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
		  }
		  PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][15]);
  	}
  	if(PlayerInfo[ playerid ][ xHunger ] >= (30) && PlayerInfo[ playerid ][ xHunger ] <= (60))
  	{
           for(new i = 11; i < 14; ++i)
		   {
				 PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
		   }
           PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][15]);
           PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][14]);
  	}
  	if(PlayerInfo[ playerid ][ xHunger ] > (0) && PlayerInfo[ playerid ][ xHunger ] <= (30))
  	{
           for(new i = 11; i < 13; ++i)
		   {
				PlayerTextDrawShow(playerid,Player_TDs[playerid][SM_Info_TDs][i]);
		   }
           PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][15]);
		   PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][14]);
	       PlayerTextDrawHide(playerid,Player_TDs[playerid][SM_Info_TDs][13]);
  	}
	return (true);
}

