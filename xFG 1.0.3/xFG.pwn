    if( IsPlayerAttachedObjectSlotUsed( playerid, OBJECT_SLOT_RHAND ) ) RemovePlayerAttachedObject( playerid, OBJECT_SLOT_RHAND );
	ClearAnimations( playerid );
	return (true);
}

